{
  "compileOnSave": true,
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "incremental": true,
    "module": "ESNext",
    "target": "ES2020",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noUnusedLocals": true
  },
  "typeAcquisition": {
    "enable": true
  }
}
