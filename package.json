{
  "name": "giterm",
  "version": "0.3.0",
  "description": "A git gui for terminal lovers",
  "homepage": "https://github.com/Nick-Lucas/giterm",
  "main": "init.js",
  "author": {
    "name": "Nick Lucas",
    "email": "me@nicklucas.co.uk"
  },
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run private:compile -- --source-maps true && run-p -r private:watch private:serve",
    "test": "jest --watch \\.*\\.test.js\\",
    "lint": "eslint --no-ignore scripts app *.js",
    "lint:fix": "eslint --no-ignore scripts app *.js --fix",
    "check-format": "npm run private:format -- --list-different",
    "pack": "run-s private:clean private:compile private:build:all",
    "pack:mac": "run-s private:clean private:compile private:build:mac",
    "pack:win": "run-s private:clean private:compile private:build:win",
    "pack:linux": "run-s private:clean private:compile private:build:linux",
    "postinstall": "electron-builder install-app-deps",
    "private:build:all": "electron-builder -mwl",
    "private:build:mac": "electron-builder --mac",
    "private:build:win": "electron-builder --win",
    "private:build:linux": "electron-builder --linux",
    "private:watch": "npm run private:compile -- --source-maps true --watch --skip-initial-build",
    "private:serve": "babel-node scripts/serve.js",
    "private:compile": "babel app/ --ignore **/gitgraph/node_modules --copy-files --out-dir build",
    "private:clean": "rimraf build",
    "private:format": "prettier \"scripts/*.js\" \"app/**/*.js\" \"test/**/*.js\""
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "debounce": "^1.2.0",
    "feather-icons": "^4.7.3",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "node-pty": "^0.9.0",
    "nodegit": "^0.26.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-electron-contextmenu": "^1.0.0",
    "react-feather": "^1.1.1",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-svg-pathline": "^0.5.0",
    "react-virtualized": "^9.21.2",
    "redux": "^4.0.4",
    "redux-actions": "^2.2.1",
    "redux-localstorage": "^0.4.1",
    "redux-saga": "^1.1.3",
    "simple-git": "^1.129.0",
    "styled-components": "^4.4.1",
    "tslib": "^1.9.3",
    "xterm": "^4.3.0",
    "xterm-addon-fit": "^0.3.0",
    "xterm-addon-web-links": "^0.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@types/jest": "^24.0.23",
    "babel-eslint": "^10.0.3",
    "browser-sync": "^2.23.6",
    "electron": "^7.1.5",
    "electron-builder": "^21.2.0",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.0.1",
    "prettier": "^1.13.7",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.5.4"
  }
}
