{
  "name": "giterm",
  "version": "0.14.0",
  "description": "A git gui for terminal lovers",
  "homepage": "https://github.com/Nick-Lucas/giterm",
  "main": "init.js",
  "author": {
    "name": "Nick Lucas",
    "email": "me@nicklucas.co.uk"
  },
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "npm run private:compile -- --source-maps true && run-p -r private:watch private:serve",
    "dev:nowatch": "NOWATCH=true npm run private:compile -- --source-maps true && npm run private:serve",
    "check-format": "npm run private:format -- --list-different",
    "pack:mac": "run-s private:clean private:compile private:build:mac",
    "pack:win": "run-s private:clean private:compile private:build:win",
    "pack:linux": "run-s private:clean private:compile private:build:linux",
    "postinstall": "run-script-os --no-arguments",
    "postinstall:nix": "if [ -z ${SKIP_NATIVE_BUILD+x} ]; then yarn run electron-builder install-app-deps; else echo \"Postinstall skipped\"; fi;",
    "postinstall:windows": "electron-builder install-app-deps",
    "private:build:mac": "electron-builder --mac --publish=never",
    "private:build:win": "electron-builder --win --publish=never",
    "private:build:linux": "electron-builder --linux --publish=never",
    "private:watch": "npm run private:compile -- --source-maps true --watch --skip-initial-build",
    "private:serve": "babel-node scripts/serve.js",
    "private:compile": "babel app/ --copy-files --out-dir build",
    "private:clean": "rimraf build",
    "private:format": "prettier \"scripts/*.js\" \"app/**/*.js\" \"test/**/*.js\""
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "@giterm/git": "1.0.0",
    "@giterm/gitgraph": "1.0.0",
    "@sentry/cli": "^1.55.2",
    "@sentry/electron": "^2.0.0",
    "@sentry/react": "^5.22.3",
    "chokidar": "^3.3.1",
    "color": "^3.1.2",
    "debounce": "^1.2.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-log": "^4.0.6",
    "electron-updater": "^4.2.2",
    "feather-icons": "^4.7.3",
    "history": "^4.10.1",
    "immer": "^7.0.9",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "node-pty": "^0.9.0",
    "nodegit": "^0.26.5",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-feather": "^1.1.1",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-svg-pathline": "^0.5.0",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.5",
    "react-window-infinite-loader": "^1.0.5",
    "redux": "^4.0.4",
    "redux-actions": "^2.2.1",
    "redux-localstorage": "^0.4.1",
    "redux-saga": "^1.1.3",
    "simple-git": "^1.129.0",
    "styled-components": "^4.4.1",
    "tslib": "^1.9.3",
    "xterm": "^4.8.1",
    "xterm-addon-fit": "^0.4.0",
    "xterm-addon-web-links": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/jest": "^24.0.23",
    "babel-plugin-module-resolver": "^4.0.0",
    "browser-sync": "^2.23.6",
    "electron": "8.4.0",
    "electron-builder": "^22.7.0",
    "electron-notarize": "^1.0.0",
    "npm-run-all": "^4.0.1",
    "prettier": "^1.13.7",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^3.0.2"
  }
}
