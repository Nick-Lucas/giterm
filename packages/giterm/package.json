{
  "name": "giterm",
  "version": "0.17.1",
  "description": "A git gui for terminal lovers",
  "homepage": "https://github.com/Nick-Lucas/giterm",
  "main": "init.js",
  "author": {
    "name": "Nick Lucas",
    "email": "me@nicklucas.co.uk"
  },
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "yarn run private:compile && run-p -r private:watch private:serve",
    "dev:nowatch": "NOWATCH=true yarn run private:compile && yarn run private:serve",
    "pack:mac": "run-s private:clean private:compile private:build:mac",
    "pack:win": "run-s private:clean private:compile private:build:win",
    "pack:linux": "run-s private:clean private:compile private:build:linux",
    "postinstall": "run-script-os",
    "postinstall:nix": "if [ -z ${SKIP_NATIVE_BUILD+x} ]; then yarn run electron-builder install-app-deps; else echo \"Postinstall skipped\"; fi;",
    "postinstall:windows": "electron-builder install-app-deps",
    "private:build:mac": "electron-builder --mac --publish=never",
    "private:build:win": "electron-builder --win --publish=never",
    "private:build:linux": "electron-builder --linux --publish=never",
    "private:watch": "yarn run private:compile --watch --skip-initial-build",
    "private:serve": "babel-node scripts/serve.js",
    "private:compile": "babel app/ --extensions=.ts,.tsx,.js --copy-files --out-dir build/ --source-maps",
    "private:clean": "rimraf build"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "@electron/remote": "^2.0.1",
    "@giterm/git": "1.0.0",
    "@giterm/gitgraph": "1.0.0",
    "@monaco-editor/react": "^4.3.1",
    "@sentry/cli": "^1.68.0",
    "@sentry/electron": "^2.5.3",
    "@sentry/react": "^6.12.0",
    "@types/react-virtualized-auto-sizer": "~1.0.1",
    "@types/react-window-infinite-loader": "~1.0.5",
    "chokidar": "^3.3.1",
    "color": "^3.1.2",
    "debounce": "^1.2.0",
    "diff2html": "^3.4.11",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.0.6",
    "electron-updater": "^4.2.2",
    "feather-icons": "^4.7.3",
    "history": "^4.10.1",
    "immer": "^7.0.9",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "monaco-editor": "^0.29.1",
    "node-pty": "^0.10.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-feather": "^1.1.1",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "react-svg-pathline": "^0.5.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "react-window-infinite-loader": "^1.0.7",
    "redux": "^4.0.4",
    "redux-actions": "^2.2.1",
    "redux-localstorage": "^0.4.1",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.1",
    "tslib": "^2.3.1",
    "xterm": "^4.14.1",
    "xterm-addon-fit": "^0.5.0",
    "xterm-addon-web-links": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.15.0",
    "@types/color": "3",
    "@types/history": "^4.7.9",
    "@types/jest": "^24.0.23",
    "@types/react-dom": "^16.9.14",
    "@types/react-router": "5.1.2",
    "@types/react-router-redux": "^4.0.53",
    "@types/react-window": "^1.8.5",
    "@types/styled-components": "^5.1.15",
    "babel-plugin-module-resolver": "^4.0.0",
    "browser-sync": "^2.23.6",
    "electron": "^15.2.0",
    "electron-builder": "^22.13.1",
    "electron-notarize": "^1.1.1",
    "npm-run-all": "^4.0.1",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^3.0.2"
  }
}
